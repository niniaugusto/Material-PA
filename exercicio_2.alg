algoritmo "positivo e negativo"
// Autor : Nicolas Augusto da Silva
// Data : 02/06/2023

var
salario: real
gasto: real


inicio
escreval ("digite salario: ")
leia (salario)
escreval ("digite gasto: ")
leia (gasto)
se salario >= gasto entao
escreval ("saldo positivo")
senao
escreval ("saldo negativo")
fimse


escreval ("acabou")

fimalgoritmo
